import string.ei;
import int.ei;

@dont_fix_variant = true;
@notConstructable = true;
pub class vector<Type> {
    @description = "Get type";
    @example = "
        v vec : [int, *] = [12];
        print(vec.getType()); //vector
    ";
    pub fn getType() : string {
        ret "vector";
    }

    @description = "Get size of vector";
    @native = true;
    @example = "
        v vec : [int, *] = [12];
        print(vec.len()); //1
    ";
    pub fn len() : int;
}